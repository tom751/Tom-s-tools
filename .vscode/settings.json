{
  "files.exclude": {
      "out": false
  },
  "search.exclude": {
      "out": true
  },
  "typescript.tsc.autoDetect": "off",
  "editor.defaultFormatter": "biomejs.biome",
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "editor.codeActionsOnSave": {
    "quickfix.biome": true,
    "source.organizeImports.biome": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "biomejs.biome"
  }
}